#include <stdio.h>
#define MAX 100
int knapSack(int W, int wt[], int val[], int n) {
    int i, w;
    int K[n+1][W+1];

    for (i = 0; i <= n; i++) {
        for (w = 0; w <= W; w++) {
            if (i == 0 || w == 0)
                K[i][w] = 0;
            else if (wt[i-1] <= w)
                K[i][w] = (val[i-1] > (K[i-1][w-wt[i-1]] + val[i-1])) ? val[i-1] : K[i-1][w];
            else
                K[i][w] = K[i-1][w];
        }
    }
    return K[n][W];
}

int main() {
    int val[] = {60, 100, 120};
    int wt[] = {10, 20, 30};
    int W = 50;
    int n = sizeof(val) / sizeof(val[0]);
    printf("192211645 G.PRIYANKAREDDY\n");
    printf("Maximum value in Knapsack = %d\n", knapSack(W, wt, val, n));

    return 0;
}
