#include <stdio.h>
int main() {
	printf("192211645 PRIYANKA REDDY\n");
    int t1 = 0, t2 = 1, nextTerm = 0, n;

    printf("Enter the number of terms: ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");

    for (int i = 1; i <= n; ++i) {
        printf("%d, ", t1);
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
    } 
    return 0;
}
